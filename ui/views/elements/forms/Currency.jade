.form-group(class!="<%= className%>")
	label.control-label <%= displayName%>
	.input-group
		span.input-group-addon <%= locale.app.currencySymbol %>
		input.form-control(id!="<%= elemName %>",name!="<%= elemName %>",type="text",value!="<%= displayValue %>",placeholder!="<%= placeholder %>" )
		span.input-group-addon .00